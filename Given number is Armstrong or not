#include <stdio.h>
int main()
{
	int n,m,sum=0,rem;
	printf("Enter The Number:");
	scanf("%d",&n);
	m=n;
	while(n!=0)
	{
		rem=n%10;
		sum=sum+(rem*rem*rem);
		n/=10;
	}
	if(m==sum)
	{
		printf("Entered Number Is an Armstrong Number");
	}
	else
	{
		printf("Entered Number Is Not an Armstrong Number");
	}
}
